export interface ScryptedStatic {
    deviceManager: DeviceManager,
    scriptSettings: Settings,
    log: Logger,
    /**
     * Create a MediaObject.
     * The mime type needs to be provided up front, but the data can be a
     * Uri string, Buffer, or a Promise for a Uri string or Buffer.
     * @param mimeType The mime type of the media. May be a wildcard, such as image/*.
     * @param data  The media data. Currently A uri string or a Buffer.
     */
    createMediaObject(data: string|Buffer|Promise<string|Buffer>, mimeType: string): MediaObject;
    /**
     * Convert a media object to a buffer of a given mime type.
     * @param mediaObject 
     * @param mimeType The target mime type of the Buffer.
     */
    convertMediaObjectToBuffer(mediaObject: MediaObject, mimeType: string): Promise<Buffer>,
    /**
     * Convert a media object to a publically accessible uri that serves
     * a media file of the given mime type.
     * @param mediaObject 
     * @param mimeType The target mime type of the Uri.
     */
    convertMediaObjectToUri(mediaObject: MediaObject, mimeType: string): Promise<string>;
}
